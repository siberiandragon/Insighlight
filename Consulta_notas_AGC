select distinct PCNFSAID.NUMNOTA as NOTA, VW_COMISSAO_AGENCIADA.CODCONTATO, VW_COMISSAO_AGENCIADA.NOMECONTATO as AGC, PCNFSAID.CODUSUR as RCA, PCUSUARI.NOME, PCNFSAID.CODCLI, PCCLIENT.CLIENTE, PCNFSAID.VLTOTAL, VW_COMISSAO_AGENCIADA.COMISSAO_AGENCIADOR as COMISSAO
from PCNFSAID
join VW_COMISSAO_AGENCIADA on PCNFSAID.NUMNOTA = VW_COMISSAO_AGENCIADA.NUMNOTA
join PCPEDC on PCNFSAID.NUMNOTA = PCPEDC.NUMNOTA
join PCCLIENT on PCNFSAID.CODCLI = PCCLIENT.CODCLI
join PCUSUARI on PCNFSAID.CODUSUR = PCUSUARI.CODUSUR
where 
PCNFSAID.DTSAIDA
between
'01/05/2023'
and 
'31/07/2023'
and 
PCNFSAID.CODFILIALNF in ('10','11','12')
and  
VW_COMISSAO_AGENCIADA.CODVENDEDOR in  ('9999')
and
VW_COMISSAO_AGENCIADA.CODCONTATO in (select CODCONTATO from PCCONTATO where CARGO in ('9999') )   
